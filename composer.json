{
    "name": "brandon/shaiyacms",
    "description": "ShaiyaCMS",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Brandon Gonzalez",
            "email": "lunethdev@gmail.com"
        }
    ],
    "require": {
      "jenssegers/blade": "dev-master",
      "illuminate/database": "^7.2",
      "vlucas/phpdotenv": "^4.1@dev",
      "phpmailer/phpmailer": "dev-master",
      "cbschuld/browser.php": "^1.9@dev",
      "pecee/simple-router": "dev-master",
      "monolog/monolog": "2.x-dev"
    },
    "minimum-stability": "dev",
    "config": {
      "optimize-autoloader": true
    },
    "autoload": {
      "psr-4": {
        "DB\\"        : "app/classes/db",
        "Utils\\"		  :	"app/classes/Utils",
        "Core\\"      : "framework/core",
        "Sys\\"       : "app/classes/sys",
        "Exception\\": "app/exception",
        "Exceptions\\" : "app/classes/exceptions",
        "Controllers\\": "app/controllers",
        "Models\\"    : "app/models",
        "Blade\\"     : "framework/blade",
        "Security\\"  : "app/classes/Security",
        "Shaiya\\"  : "app/classes/Shaiya",
        "Framework\\"  : "app/classes/Framework",
        "Donate\\"  : "app/classes/Donate"
      }
    }
}
